    $('#diagnosticoBusqueda').on('click', '.btn-buscar', function() {

	    if ($("#diagnosticoTxt").val().length >= 3) {
		    $('#diagnosticoBusquedaLista').empty();
		    $('#diagnosticoBusquedaAlert').text('');
		    $.ajax({
			    url: '@Url.Action("BuscarDiagnosticos", "NotaEvolucion")',
