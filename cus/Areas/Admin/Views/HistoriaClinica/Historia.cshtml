                    var contenido = spanElement.textContent; // Obtener el contenido del <span>
                    if (contenido === "true") {
                        spanElement.textContent = "seleccionado";
                    } else if (contenido === "false") {
                        spanElement.textContent = "no seleccionado";
                    }
